<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.aplikasiku.moviescatalogue">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_movie"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_movie"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DetailMovieActivity"
            android:parentActivityName=".activity.DetailMovieActivity"/>
        <activity android:name=".activity.DetailTvShowActivity"
            android:parentActivityName=".activity.DetailTvShowActivity"/>
    </application>

</manifest>